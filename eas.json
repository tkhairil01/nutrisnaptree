{
  "cli": {
    "version": ">=3.13.0",
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview" // Pindahkan channel ke dalam profile "preview"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development" // Pindahkan channel ke dalam profile "development"
    },
    "production": {
      "channel": "production" // Tambahkan channel untuk profile "production"
    }
  }
  // Hapus sepenuhnya bagian "update" yang sebelumnya ada di sini
}