{
  "expo": {
    "name": "nutrikoala",
    "slug": "nutrisnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kh.pwa.nutrisnap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kh.pwa.nutrisnap",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1234567890123456~9876543210"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34, // Direkomendasikan untuk SDK 51/RN 0.74
            "targetSdkVersion": 34,  // Direkomendasikan untuk SDK 51/RN 0.74
            "minSdkVersion": 24,     // Minimum API Level yang kompatibel
            "kotlinVersion": "1.9.0", // Versi Kotlin yang kompatibel
            "gradleVersion": "8.10.2" // Targetkan Gradle 8.10.2, yang menyebabkan masalah awal kita, tapi RN 0.74 seharusnya kompatibel
          },
          "ios": {
            "deploymentTarget": "13.4" // Pertahankan 13.4 karena requirement EAS/Tools
          }
        }
      ]
    ],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/73a5dff1-c044-46a3-aea5-09e338ae550f"
    },
    "extra": {
      "eas": {
        "projectId": "73a5dff1-c044-46a3-aea5-09e338ae550f"
      }
    }
  }
}