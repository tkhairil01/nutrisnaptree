{
  "expo": {
    "name": "nutrikoala",
    "slug": "nutrisnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kh.pwa.nutrisnap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kh.pwa.nutrisnap",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1234567890123456~9876543210"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33, // Sesuai dengan SDK 49/RN 0.72
            "targetSdkVersion": 33,  // Sesuai dengan SDK 49/RN 0.72
            "minSdkVersion": 23,     // Versi minimum API yang direkomendasikan
            "kotlinVersion": "1.8.0", // Versi Kotlin yang kompatibel
            "gradleVersion": "7.6.1"  // Coba paksa Gradle 7.6.1, yang lebih stabil
          },
          "ios": {
            "deploymentTarget": "13.4" // Sesuai dengan SDK 49/RN 0.72
          }
        }
      ]
    ],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/73a5dff1-c044-46a3-aea5-09e338ae550f"
    },
    "extra": {
      "eas": {
        "projectId": "73a5dff1-c044-46a3-aea5-09e338ae550f"
      }
    }
  }
}