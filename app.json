{
  "expo": {
    "name": "nutrikoala",
    "slug": "nutrisnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kh.pwa.nutrisnap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kh.pwa.nutrisnap",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1234567890123456~9876543210"
      }
    },
    "plugins": [
      "expo-ads-admob",
      "expo-camera",
      "expo-image-picker",
      "expo-localization",
      "expo-notifications",
      "expo-updates",
      "expo-splash-screen",
      "expo-font",
      "expo-asset"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/73a5dff1-c044-46a3-aea5-09e338ae550f"
    },
    "extra": {
      "eas": {
        "projectId": "73a5dff1-c044-46a3-aea5-09e338ae550f"
      }
    },
    "sdkVersion": "49.0.0"
  }
}