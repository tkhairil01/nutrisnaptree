{
  "expo": {
    "name": "nutrikoala",
    "slug": "nutrisnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kh.pwa.nutrisnap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kh.pwa.nutrisnap",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1234567890123456~9876543210"
      }
    },
    "plugins": [
      // Plugin yang paling sering digunakan dengan SDK 49
      "expo-ads-admob",
      "expo-camera",
      "expo-image-picker",
      "expo-localization",
      "expo-notifications",
      "expo-status-bar",
      "expo-updates",
      // Plugin dasar lainnya yang mungkin ada di proyek Expo
      "expo-splash-screen",
      "expo-font",
      "expo-asset",
      // expo-build-properties sering ditambahkan secara terpisah jika perlu override versi build tools
      // tetapi bukan plugin default di setiap proyek SDK 49.
      // Jika Anda memerlukannya untuk Android SDK/Gradle spesifik, tambahkan di sini:
     [
      "expo-build-properties",
       {
      "android": {
      "compileSdkVersion": 33, // Biasanya 33 untuk SDK 49
      "targetSdkVersion": 33,  // Biasanya 33 untuk SDK 49
      "minSdkVersion": 24
     }
    }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/73a5dff1-c044-46a3-aea5-09e338ae550f"
    },
    "extra": {
      "eas": {
        "projectId": "73a5dff1-c044-46a3-aea5-09e338ae550f"
      }
    },
    "sdkVersion": "49.0.0" // Ini adalah kuncinya untuk SDK 49
  }
}